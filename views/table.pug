  body
    script(src='javascripts/tabledata.js')
    script(src='javascripts/edit.js')
    table.example.display(style='width:100%')
      thead
        tr
          th Outward No
          th Reason
          th Date
          th Time
          th source
          th Destination
          th vehicle no
          th Vehicle Type
          th Person Name
          th person No
          th Contact No
          th Insructed By
          th Remarks
          th action
      tbody
        each member in data
          tr
            td #{member.outwardno}
            td #{member.reason}
            td #{member.date}
            td #{member.time}
            td #{member.from}
            td #{member.to}
            td #{member.vno}
            td #{member.vtype}
            td #{member.name1}
            td #{member.pno}
            td #{member.cno}
            td #{member.splperson}
            td #{member.remarks}
            td
              button(value=member._id style="margin-left:8px").btn.btn-danger.remove remove
      
    

      
        
          
           

                    
